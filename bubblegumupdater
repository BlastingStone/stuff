local eggs = {}
local egg3 = {}
for i,v in pairs(workspace.Eggs:GetChildren()) do 
    if not table.find(eggs,v.Name) then 
        table.insert(eggs,v.Name)
    end
end
for i,v in pairs(workspace.Eggs:GetChildren()) do 
    if not table.find(egg3,v.Name) then 
        table.insert(egg3,v.Name)
    end
end


