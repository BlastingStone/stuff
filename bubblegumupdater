    _G.EggValue = value
end)

PTE:addToggle("Auto Open Egg",false,function(value)
    _G.AutoOpenEgg = value
    while _G.AutoOpenEgg do 
        local v1 = "PurchaseEgg"
        local v2 = _G.EggValue 
        local rem = game:GetService("ReplicatedStorage").NetworkRemoteEvent
        rem:FireServer(v1,v2)
        wait(_G.EggDelayValue)
    end
end)

PTE:addSlider("Auto Open Egg Delay",0,0,10,function(value)
    _G.EggDelayValue = value
end)

