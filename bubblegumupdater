AFS:addToggle("Auto Candy Corn",false,function(value)
    local plr = game.Players.LocalPlayer
    local tween_s = game:service"TweenService";
    local info = TweenInfo.new(_G.CandyCornHouseSpeed,Enum.EasingStyle.Sine);
    local function tp(...)
      local tic_k = tick();
      local params = {...};
      local cframe = CFrame.new(params[1],params[2],params[3]);
      local tween,err = pcall(function()
          local tween = tween_s:Create(plr.Character["HumanoidRootPart"],info,{CFrame=cframe});
          tween:Play();
      end)
      if not tween then return err end
    end   
    _G.AutoCandyCorn = value
    while _G.AutoCandyCorn do    
        game:GetService("ReplicatedStorage").NetworkRemoteEvent:FireServer("Teleport","EventSpawn")
        wait(1.2)
        -- Bypass Tp 
        tp(2547.08691, 54.7007103, -204.918289)
        wait(0.3)
        tp(2518.271, 54.7007103, -216.139694)
        wait(0.3)
        tp(2490.47998, 54.7007103, -226.962311)
        wait(0.3)
        tp(2465.63354, 55.2007103, -238.937241)
        wait(0.3)
        -- First House 
        tp(2429.50586, 54.930336, -248.200836)
        wait(0.2)
        tp(2426.20996, 54.7007103, -248.569351)
        wait(_G.AutoWaitCandyCorn)
        -- Second House 
        tp(2419.36206, 54.7328949, -275.171265)
        wait(0.2)
        tp(2416.82739, 54.820118, -276.740387)
        wait(_G.AutoWaitCandyCorn)
        -- Bypass Teleport
        tp(2425.68921, 54.7007103, -313.782288)
        wait(0.3)
        -- Third House 
        tp(2409.00317, 55.1096649, -319.122589)
        wait(0.2)
        tp(2406.69995, 54.7510948, -319.744293)
        wait(_G.AutoWaitCandyCorn)
        -- Fourth House 
        tp(2418.03931, 54.7007103, -342.695923)
