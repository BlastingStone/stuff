    game.Players.LocalPlayer.Character.Humanoid.JumpPower = value
end)

Mods:addButton("Respawn",function()
    game.Players.LocalPlayer.Character.Humanoid.Health = 0 
    warn("DIED")
end)

-- Auto Farm Page 
local Auto = UI:addPage("Auto Farm")
local AutoFarmSection = Auto:addSection("Auto Farm")
local DelaySection = Auto:addSection("Auto Farm Delays")

AutoFarmSection:addToggle("Auto Bubbles",false,function(value)
    _G.AutoBubbles = value 
    while _G.AutoBubbles do 
        local v1 = "BlowBubble"
        local rem = game:GetService("ReplicatedStorage").NetworkRemoteEvent
        rem:FireServer(v1)
        wait(_G.DelayBubble)
    end
end)

AutoFarmSection:addToggle("Auto Sell",false,function(value)
    _G.AutoSell = value 
    while _G.AutoSell do 
        local v1 = "Teleport"
        local v2 = "Sell"
        local rem = game:GetService("ReplicatedStorage").NetworkRemoteEvent
        rem:FireServer(v1, v2)
        wait(_G.DelaySell)
    end
end)

AutoFarmSection:addToggle("Auto Candy Corn",false,function(value)
    _G.AutoCandyCorn = value 
    while _G.AutoCandyCorn do 
        local v3 = "Teleport"
        local v4 = "EventSpawn"
        local rem = game:GetService("ReplicatedStorage").NetworkRemoteEvent
        rem:FireServer(v3, v4)
        wait(1)
        tp(2428.83618, 55.0537605, -248.497726)
        wait(3)
        tp(2421.25537, 54.7258492, -276.733337)
        wait(3)
        tp(2409.83398, 54.9123192, -318.450043)
        wait(3)
        tp(2421.0979, 54.7007103, -341.316223)
        wait(3)
        tp(2492.46094, 54.7953873, -339.586975)
        wait(3)
        tp(2538.28491, 54.7118912, -320.481537)
        wait(3)
        tp(2561.58887, 54.710968, -322.130035)
        wait(3)
    end
end)


AutoFarmSection:addToggle("Auto Farm All (Toggle Auto Bubbles for this)",false,function(value)
    _G.AutoFarmAll = value 
    while _G.AutoFarmAll do 
        local v3 = "Teleport"
        local v4 = "EventSpawn"
        local rem = game:GetService("ReplicatedStorage").NetworkRemoteEvent
        rem:FireServer(v3, v4)
        wait(0.7)
        -- Starting Candy Corn Farm 
        tp(2428.83618, 55.0537605, -248.497726)
        wait(3)
        tp(2421.25537, 54.7258492, -276.733337)
        wait(3)
        tp(2409.83398, 54.9123192, -318.450043)
        wait(3)
        tp(2421.0979, 54.7007103, -341.316223)
