 local plr = game.Players.LocalPlayer
    local tween_s = game:service"TweenService";
    local info = TweenInfo.new(4,Enum.EasingStyle.Sine);
    local function tp(...)
      local tic_k = tick();
      local params = {...};
      local cframe = CFrame.new(params[1],params[2],params[3]);
      local tween,err = pcall(function()
          local tween = tween_s:Create(plr.Character["HumanoidRootPart"],info,{CFrame=cframe});
          tween:Play();
      end)
      if not tween then return err end
end


-- Varaibles 
local PS = game:GetService("Players")
local CS = game:GetService("CoreGui")

-- Tables
local eggs = {}
for i,v in pairs(workspace.Eggs:GetChildren()) do 
    if not table.find(eggs,v.Name) then 
        table.insert(eggs,v.Name)
    end
end

-- Check Replication 
local BubbleREP = CS:FindFirstChild("Bubble Gum Simulator V4")
if BubbleREP then BubbleREP:Destroy() end 

-- Bubble Gum Simulator
local GUI = loadstring(game:HttpGet("https://pastebin.com/raw/qwdPKKDN"))()
local UI = GUI.new("Bubble Gum Simulator V4")

-- Themes 
local themes = {
    Background = Color3.fromRGB(24,24,24),
    Glow = Color3.fromRGB(0,0,0),
    Accent = Color3.fromRGB(10,10,10),
    LightContrast = Color3.fromRGB(20,20,20),
    DarkContrast = Color3.fromRGB(14,14,14),
    TextColor = Color3.fromRGB(255,255,255)
}

