CloseButton.Name = "CloseButton"
CloseButton.Parent = MainFrame
CloseButton.BackgroundColor3 = Color3.new(0.313726, 0.313726, 0.313726)
CloseButton.BackgroundTransparency = 1
CloseButton.BorderSizePixel = 0
CloseButton.Position = UDim2.new(0, 231, 0, 8)
CloseButton.Size = UDim2.new(0, 44, 0, 33)
CloseButton.Font = Enum.Font.SourceSans
CloseButton.Text = "x"
CloseButton.TextColor3 = Color3.new(0, 0, 0)
CloseButton.TextSize = 14


LogInButton.MouseButton1Click:Connect(function()
	if UsernameBox.Text == "BytesTheHotMan" and PasswordBox.Text == "coolkidsintown38" then 
		LogInButton.Text = "Loading..."
		wait(.3)
		LogInButton.Text = "Loading.."
		wait(.3)
		LogInButton.Text = "Loading."
		wait(.3)
		LogInButton.Text = "Loading..."
		wait(.5)
		LogInButton.Text = "Done Loading !!"
		wait(0.5)
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Bytes77/stuff/main/TestersScript"))()
		warn("Correct Information")
		WTFTESTHOW:Destroy()
	else
		warn("Wrong password")
		LogInButton.Text = "Try again"
		wait(1)
		LogInButton.Text = "Login"
	end
end)

CloseButton.MouseButton1Click:Connect(function()
	WTFTESTHOW:Destroy()
	warn("Login in ui destroyed")
end)
