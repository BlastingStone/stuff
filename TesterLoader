UsernameBox.Size = UDim2.new(0, 200, 0, 50)
UsernameBox.Font = Enum.Font.SourceSans
UsernameBox.Text = "Username"
UsernameBox.TextColor3 = Color3.new(1, 1, 1)
UsernameBox.TextSize = 20

PasswordBox.Name = "PasswordBox"
PasswordBox.Parent = MainFrame
PasswordBox.BackgroundColor3 = Color3.new(0.380392, 0.380392, 0.380392)
PasswordBox.BorderColor3 = Color3.new(0.313726, 0.313726, 0.313726)
