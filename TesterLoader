LogInLabel.Size = UDim2.new(0, 286, 0, 50)
LogInLabel.Font = Enum.Font.SourceSans
LogInLabel.Text = "Log In : "
LogInLabel.TextColor3 = Color3.new(1, 1, 1)
LogInLabel.TextSize = 30
LogInLabel.TextWrapped = true

UsernameBox.Name = "UsernameBox"
UsernameBox.Parent = MainFrame
UsernameBox.BackgroundColor3 = Color3.new(0.380392, 0.380392, 0.380392)
UsernameBox.BorderColor3 = Color3.new(0.313726, 0.313726, 0.313726)
UsernameBox.BorderSizePixel = 0
